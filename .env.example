# ðŸš€ SWARM MULTI-AGENT SYSTEM - LOCAL DEVELOPMENT ENVIRONMENT
# Copy this to .env and update with your actual credentials

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True
SECRET_KEY=dev-secret-key-change-in-production

# Database Configuration (SQLite for development)
DATABASE_URL=sqlite:///swarm_agents.db

# OpenRouter API Configuration
OPENROUTER_API_KEY=your-openrouter-api-key-here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# SuperMemory Configuration
SUPERMEMORY_API_KEY=your-supermemory-api-key-here
SUPERMEMORY_BASE_URL=https://api.supermemory.ai

# Mailgun Configuration
MAILGUN_API_KEY=your-mailgun-api-key-here
MAILGUN_DOMAIN=your-domain.com
MAILGUN_BASE_URL=https://api.mailgun.net/v3

# CORS Configuration
CORS_ORIGINS=http://localhost:5173,http://127.0.0.1:5173,https://localhost:5173

# Server Configuration
HOST=0.0.0.0
PORT=5001

# Security Configuration
JWT_SECRET_KEY=dev-jwt-secret-key-change-in-production
BCRYPT_LOG_ROUNDS=4

# Logging Configuration
LOG_LEVEL=DEBUG
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# Rate Limiting
RATE_LIMIT_ENABLED=False
RATE_LIMIT_DEFAULT=1000 per hour

# Performance Configuration
WORKERS=1
TIMEOUT=30
KEEPALIVE=2

# Feature Flags
ENABLE_WEBSOCKET=True
ENABLE_REAL_TIME=True
ENABLE_ANALYTICS=False
ENABLE_CACHING=False

